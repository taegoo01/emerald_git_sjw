package com.javalec.single;

public class Student {

	public String name;
	public int age;
	
	public Student(String name, int age) {
		this.name = name;
		this.age = age;
	}
	
	public void getStatus() {
		System.out.println("이름:" + name + "나이:" + age);
	}

	public String getName() {
		return name;
	}

	public int getAge() {
		return age;
	}
	
	
	
}
